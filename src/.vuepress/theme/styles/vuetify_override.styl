.pl-logo
  margin: auto

.v-toolbar__extension
  background-color: #263238

.v-toolbar__content > *:last-child
  margin auto

@media only screen and (min-width: 1904px)
  .container
  .submenu_container
  .toolbar_container
    max-width 1200px



@import '~vuetify/src/stylus/app.styl'

code
  box-shadow: unset

.right
  float: unset !important

ul, ol
  padding 0 0 24px 20px

code
  &:before, &:after
    content ''
  font-weight 400
